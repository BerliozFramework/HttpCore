<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Berlioz Debug Console">
    <meta name="author" content="Berlioz Framework">

    <title>Berlioz Debug Console / Report #{{ report.uniqid }}</title>

    {{ entrypoints('debug', 'css', {}, entrypoints) }}
</head>

<body class="console" data-report="{{ report.uniqid }}">
<div id="loader-wrapper" style="display: none;">
    <div id="loader"></div>
    <svg id="loader-logo" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 500 500">
        <title>Berlioz Framework</title>
        <path d="M250,0C111.93,0,0,111.93,0,250A250,250,0,0,0,141.28,475.18c14.54-25.75,36-64.21,42.81-78.36a88.54,88.54,0,0,0,8.23-40.48c-0.43-18.8-7.11-27.61-40.67-82.78-21.42-35.21-16.53-29.67-21.42-35.21-4.49-5.09-4.85-3.09-4.16,22.49,0.7,25.92-5,63.54-12.73,81.75-1.12,2.63-7.37,17.72-13.6,16.87s-6.23-21.73-18.49-36.82c-6.83-8.4-17.34-14.69-29.45-26.9A19.49,19.49,0,0,1,46,285.63a17.89,17.89,0,0,1,.6-8.6c24.29-61,42.24-87.92,54.58-101,7.93-8.39,18.39-16.17,25.52-32.79,4.77-11.13,5.35-19.91,13.74-26.59,6.35-5.05,11.75-4.58,22.06-6.56,30.18-5.81,34.51-19.62,71.09-28,9.26-2.12,12.24-.6,16.39-0.6s7.13-1.52,16.39.6c36.58,8.36,40.91,22.18,71.09,28,10.31,2,15.71,1.51,22.06,6.56,8.39,6.67,9,15.46,13.74,26.59,7.13,16.62,17.58,24.4,25.52,32.79,12.35,13.05,30.29,40,54.58,101a17.89,17.89,0,0,1,.6,8.6,19.49,19.49,0,0,1-5.77,10.13c-12.11,12.22-22.62,18.5-29.45,26.9-12.27,15.09-12.18,36-18.49,36.82s-12.48-14.24-13.6-16.87c-7.76-18.21-13.43-55.84-12.73-81.75,0.69-25.58.33-27.58-4.16-22.49-4.89,5.54,0,0-21.42,35.21-33.56,55.17-40.25,64-40.67,82.78a88.54,88.54,0,0,0,8.23,40.48c6.73,14.07,28.7,52.15,43.74,77.9A250,250,0,0,0,500,250C500,111.93,388.07,0,250,0Z"></path>
    </svg>
</div>

<nav class="navbar navbar-dark fixed-top bg-dark flex-md-nowrap p-0 pr-2 shadow">
    <div class="navbar-brand col col-md-3 col-lg-2 mr-auto">
        <svg id="logo" class="d-inline-block" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 500 500">
            <title>Berlioz Framework</title>
            <path d="M250,0C111.93,0,0,111.93,0,250A250,250,0,0,0,141.28,475.18c14.54-25.75,36-64.21,42.81-78.36a88.54,88.54,0,0,0,8.23-40.48c-0.43-18.8-7.11-27.61-40.67-82.78-21.42-35.21-16.53-29.67-21.42-35.21-4.49-5.09-4.85-3.09-4.16,22.49,0.7,25.92-5,63.54-12.73,81.75-1.12,2.63-7.37,17.72-13.6,16.87s-6.23-21.73-18.49-36.82c-6.83-8.4-17.34-14.69-29.45-26.9A19.49,19.49,0,0,1,46,285.63a17.89,17.89,0,0,1,.6-8.6c24.29-61,42.24-87.92,54.58-101,7.93-8.39,18.39-16.17,25.52-32.79,4.77-11.13,5.35-19.91,13.74-26.59,6.35-5.05,11.75-4.58,22.06-6.56,30.18-5.81,34.51-19.62,71.09-28,9.26-2.12,12.24-.6,16.39-0.6s7.13-1.52,16.39.6c36.58,8.36,40.91,22.18,71.09,28,10.31,2,15.71,1.51,22.06,6.56,8.39,6.67,9,15.46,13.74,26.59,7.13,16.62,17.58,24.4,25.52,32.79,12.35,13.05,30.29,40,54.58,101a17.89,17.89,0,0,1,.6,8.6,19.49,19.49,0,0,1-5.77,10.13c-12.11,12.22-22.62,18.5-29.45,26.9-12.27,15.09-12.18,36-18.49,36.82s-12.48-14.24-13.6-16.87c-7.76-18.21-13.43-55.84-12.73-81.75,0.69-25.58.33-27.58-4.16-22.49-4.89,5.54,0,0-21.42,35.21-33.56,55.17-40.25,64-40.67,82.78a88.54,88.54,0,0,0,8.23,40.48c6.73,14.07,28.7,52.15,43.74,77.9A250,250,0,0,0,500,250C500,111.93,388.07,0,250,0Z"></path>
        </svg>
        Berlioz Debug
    </div>

    <button class="navbar-toggler border-0 d-md-none" type="button" data-target="#sidebar" data-toggle="collapse" aria-expanded="true">
        <span class="navbar-toggler-icon"></span>
    </button>
    <button class="navbar-toggler border-0 d-none" type="button" data-toggle="berlioz-console-new-window" aria-expanded="false">
        <svg class="feather feather-external-link" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true">
            <path d="M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6"></path>
            <polyline points="15 3 21 3 21 9"></polyline>
            <line x1="10" y1="14" x2="21" y2="3"></line>
        </svg>
    </button>
    <button class="navbar-toggler border-0 d-none" type="button" data-dismiss="berlioz-console" aria-expanded="false">
        <svg class="feather feather-x" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true">
            <line x1="18" y1="6" x2="6" y2="18"></line>
            <line x1="6" y1="6" x2="18" y2="18"></line>
        </svg>
    </button>
</nav>

<div class="container-fluid">
    <div class="row">
        <nav class="col-md-3 col-lg-2 bg-light sidebar collapse" id="sidebar" style="">
            <div class="sidebar-sticky">
                <div class="sidebar-heading d-flex justify-content-between align-items-center px-3 mb-1">
                    <label for="report_id">Report</label>
                    <select id="report_id" class="custom-select">
                        <option value="{{ report.uniqid }}">#{{ report.uniqid }}</option>
                    </select>
                </div>
                <ul class="nav flex-column mb-2">
                    <li class="nav-item">
                        <a class="nav-link{% if app.route.name starts with '_berlioz/console/home' %} active{% endif %}" href="{{ path('_berlioz/console/home', {"id": report.uniqid}) }}">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-home">
                                <path d="M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z"></path>
                                <polyline points="9 22 9 12 15 12 15 22"></polyline>
                            </svg>
                            Dashboard
                            {% if app.route.name starts with '_berlioz/console/home' %}
                                <span class="sr-only">(current)</span>
                            {% endif %}
                        </a>
                    </li>
                </ul>
                <ul class="nav flex-column my-2">
                    <li class="nav-item">
                        <a class="nav-link{% if app.route.name starts with '_berlioz/console/environment' %} active{% endif %}" href="{{ path('_berlioz/console/environment', {"id": report.uniqid}) }}">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="feather feather-server">
                                <rect x="2" y="2" width="20" height="8" rx="2" ry="2"></rect>
                                <rect x="2" y="14" width="20" height="8" rx="2" ry="2"></rect>
                                <line x1="6" y1="6" x2="6.01" y2="6"></line>
                                <line x1="6" y1="18" x2="6.01" y2="18"></line>
                            </svg>
                            Environment
                            {% if app.route.name starts with '_berlioz/console/environment' %}
                                <span class="sr-only">(current)</span>
                            {% endif %}
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link{% if app.route.name starts with '_berlioz/console/performances' %} active{% endif %}" href="{{ path('_berlioz/console/performances', {"id": report.uniqid}) }}">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-cpu">
                                <rect x="4" y="4" width="16" height="16" rx="2" ry="2"></rect>
                                <rect x="9" y="9" width="6" height="6"></rect>
                                <line x1="9" y1="1" x2="9" y2="4"></line>
                                <line x1="15" y1="1" x2="15" y2="4"></line>
                                <line x1="9" y1="20" x2="9" y2="23"></line>
                                <line x1="15" y1="20" x2="15" y2="23"></line>
                                <line x1="20" y1="9" x2="23" y2="9"></line>
                                <line x1="20" y1="14" x2="23" y2="14"></line>
                                <line x1="1" y1="9" x2="4" y2="9"></line>
                                <line x1="1" y1="14" x2="4" y2="14"></line>
                            </svg>
                            Performances
                            {% if app.route.name starts with '_berlioz/console/performances' %}
                                <span class="sr-only">(current)</span>
                            {% endif %}
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link{% if app.route.name starts with '_berlioz/console/activities' %} active{% endif %}" href="{{ path('_berlioz/console/activities', {"id": report.uniqid}) }}">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-activity">
                                <polyline points="22 12 18 12 15 21 9 3 6 12 2 12"></polyline>
                            </svg>
                            Activities ({{ report.timeLine|length }})
                            {% if app.route.name starts with '_berlioz/console/activities' %}
                                <span class="sr-only">(current)</span>
                            {% endif %}
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link{% if app.route.name starts with '_berlioz/console/exception' %} active{% endif %}{% if report.hasExceptionThrown() %} text-danger{% endif %}" href="{{ path('_berlioz/console/exception', {"id": report.uniqid}) }}">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="feather feather-alert-circle">
                                <circle cx="12" cy="12" r="10"></circle>
                                <line x1="12" y1="8" x2="12" y2="12"></line>
                                <line x1="12" y1="16" x2="12" y2="16"></line>
                            </svg>
                            Exception
                            {% if app.route.name starts with '_berlioz/console/exception' %}
                                <span class="sr-only">(current)</span>
                            {% endif %}
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link{% if app.route.name starts with '_berlioz/console/php-errors' %} active{% endif %}{% if report.phpError|length > 0 %} text-danger{% endif %}" href="{{ path('_berlioz/console/php-errors', {"id": report.uniqid}) }}">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-alert-triangle">
                                <path d="M10.29 3.86L1.82 18a2 2 0 0 0 1.71 3h16.94a2 2 0 0 0 1.71-3L13.71 3.86a2 2 0 0 0-3.42 0z"></path>
                                <line x1="12" y1="9" x2="12" y2="13"></line>
                                <line x1="12" y1="17" x2="12" y2="17"></line>
                            </svg>
                            PHP errors ({{ report.phpError|length }})
                            {% if app.route.name starts with '_berlioz/console/php-errors' %}
                                <span class="sr-only">(current)</span>
                            {% endif %}
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link{% if app.route.name starts with '_berlioz/console/used-classes' %} active{% endif %}" href="{{ path('_berlioz/console/used-classes', {"id": report.uniqid}) }}">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-list">
                                <line x1="8" y1="6" x2="21" y2="6"></line>
                                <line x1="8" y1="12" x2="21" y2="12"></line>
                                <line x1="8" y1="18" x2="21" y2="18"></line>
                                <line x1="3" y1="6" x2="3" y2="6"></line>
                                <line x1="3" y1="12" x2="3" y2="12"></line>
                                <line x1="3" y1="18" x2="3" y2="18"></line>
                            </svg>
                            Used classes
                            {% if app.route.name starts with '_berlioz/console/used-classes' %}
                                <span class="sr-only">(current)</span>
                            {% endif %}
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link{% if app.route.name starts with '_berlioz/console/cache' %} active{% endif %}" href="{{ path('_berlioz/console/cache', {"id": report.uniqid}) }}">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-hard-drive">
                                <line x1="22" y1="12" x2="2" y2="12"></line>
                                <path d="M5.45 5.11L2 12v6a2 2 0 0 0 2 2h16a2 2 0 0 0 2-2v-6l-3.45-6.89A2 2 0 0 0 16.76 4H7.24a2 2 0 0 0-1.79 1.11z"></path>
                                <line x1="6" y1="16" x2="6.01" y2="16"></line>
                                <line x1="10" y1="16" x2="10.01" y2="16"></line>
                            </svg>
                            Cache
                            {% if app.route.name starts with '_berlioz/console/cache' %}
                                <span class="sr-only">(current)</span>
                            {% endif %}
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link{% if app.route.name starts with '_berlioz/console/config' %} active{% endif %}" href="{{ path('_berlioz/console/config', {"id": report.uniqid}) }}">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-settings">
                                <circle cx="12" cy="12" r="3"></circle>
                                <path d="M19.4 15a1.65 1.65 0 0 0 .33 1.82l.06.06a2 2 0 0 1 0 2.83 2 2 0 0 1-2.83 0l-.06-.06a1.65 1.65 0 0 0-1.82-.33 1.65 1.65 0 0 0-1 1.51V21a2 2 0 0 1-2 2 2 2 0 0 1-2-2v-.09A1.65 1.65 0 0 0 9 19.4a1.65 1.65 0 0 0-1.82.33l-.06.06a2 2 0 0 1-2.83 0 2 2 0 0 1 0-2.83l.06-.06a1.65 1.65 0 0 0 .33-1.82 1.65 1.65 0 0 0-1.51-1H3a2 2 0 0 1-2-2 2 2 0 0 1 2-2h.09A1.65 1.65 0 0 0 4.6 9a1.65 1.65 0 0 0-.33-1.82l-.06-.06a2 2 0 0 1 0-2.83 2 2 0 0 1 2.83 0l.06.06a1.65 1.65 0 0 0 1.82.33H9a1.65 1.65 0 0 0 1-1.51V3a2 2 0 0 1 2-2 2 2 0 0 1 2 2v.09a1.65 1.65 0 0 0 1 1.51 1.65 1.65 0 0 0 1.82-.33l.06-.06a2 2 0 0 1 2.83 0 2 2 0 0 1 0 2.83l-.06.06a1.65 1.65 0 0 0-.33 1.82V9a1.65 1.65 0 0 0 1.51 1H21a2 2 0 0 1 2 2 2 2 0 0 1-2 2h-.09a1.65 1.65 0 0 0-1.51 1z"></path>
                            </svg>
                            Configuration
                            {% if app.route.name starts with '_berlioz/console/config' %}
                                <span class="sr-only">(current)</span>
                            {% endif %}
                        </a>
                    </li>
                </ul>
                {% if report.sections|length > 0 %}
                    <h6 class="sidebar-heading d-flex justify-content-between align-items-center px-3 mt-3 mb-1">
                        <span>Additional sections</span>
                    </h6>
                    <ul class="nav flex-column mb-2">
                        {% with {selectedSection: section.sectionId} %}
                            {% for section in report.sections %}
                                <li class="nav-item">
                                    <a class="nav-link{% if app.route.name starts with '_berlioz/console/section' and section.sectionId == selectedSection %} active{% endif %}" href="{{ path('_berlioz/console/section', {"id": report.uniqid, "section": section.sectionId}) }}">
                                        {% if block("menu", section.templateName) is defined %}
                                            {{ block("menu", section.templateName) }}
                                        {% else %}
                                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-arrow-right-circle">
                                                <circle cx="12" cy="12" r="10"></circle>
                                                <polyline points="12 16 16 12 12 8"></polyline>
                                                <line x1="8" y1="12" x2="16" y2="12"></line>
                                            </svg>
                                            {{ section.sectionName }}
                                        {% endif %}
                                        {% if app.route.name starts with '_berlioz/console/section' and section.sectionId == selectedSection %}
                                            <span class="sr-only">(current)</span>
                                        {% endif %}
                                    </a>
                                </li>
                            {% endfor %}
                        {% endwith %}
                    </ul>
                {% endif %}
            </div>
        </nav>

        <main role="main" class="col col-md-9 col-lg-10 ml-sm-auto px-4">
            {% block main %}{% endblock %}
        </main>
    </div>
</div>

{{ entrypoints('debug', 'js', {}, entrypoints) }}
{% block inlineJscripts %}{% endblock %}
</body>
</html>
