<h4>Profile</h4>
<div class="table-responsive">
    <table class="table table-striped table-sm border-top">
        <tbody>
        <tr>
            <th scope="row" class="text-nowrap">Name</th>
            <td>{{ activity.name }}</td>
        </tr>
        <tr>
            <th scope="row" class="text-nowrap">Group</th>
            <td>{{ activity.group|default('--') }}</td>
        </tr>
        {% if activity.description %}
            <tr>
                <th scope="row" class="text-nowrap">Description</th>
                <td>{{ activity.description }}</td>
            </tr>
        {% endif %}
        <tr>
            <th scope="row" class="text-nowrap">Start after</th>
            <td>{{ ((activity.startMicroTime - snapshot.timeLine.firstTime) * 1000)|round(3) }} ms</td>
        </tr>
        <tr>
            <th scope="row" class="text-nowrap">Duration</th>
            <td>{{ (activity.duration * 1000)|round(3) }} ms</td>
        </tr>
        <tr>
            <th scope="row" class="text-nowrap">Memory usage</th>
            <td>{{ activity.memoryUsage|human_file_size }}</td>
        </tr>
        <tr>
            <th scope="row" class="text-nowrap">Memory peak usage</th>
            <td>{{ activity.memoryPeakUsage|human_file_size }}</td>
        </tr>
        </tbody>
    </table>
</div>

{% if activity.detail %}
    <h4>Detail</h4>
    <pre class="w-100"><code>{{ aDetail }}</code></pre>
{% endif %}

{% if activity.result %}
    <h4>Result</h4>
    <pre class="w-100"><code>{{ aResult }}</code></pre>
{% endif %}